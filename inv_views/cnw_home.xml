<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>


        <record id="cnw_home_tree" model="ir.ui.view">
            <field name="name">cnw.home.tree</field>
            <field name="model">cnw.home</field>
            <field name="arch" type="xml">
                <tree string="CNW Dashboard"   create="false" edit="false" delete="false" >
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="cnw_home_form" model="ir.ui.view">
            <field name="name">cnw.home.form</field>
            <field name="model">cnw.home</field>
            <field name="arch" type="xml">
                <form string="AR Dashboard"   create="false" edit="false" delete="false" >
                    <field name="name"/>
                </form>
            </field>
        </record>

        <record id="cnw_home_action" model="ir.actions.act_window">
            <field name="name"> Home</field>
            <field name="res_model">cnw.home</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p>
                    <center>
                        <strong>
                    Portal Report
                        </strong>
                    </center>
                </p>
                <p>
                    Portal Report
                </p>
            </field>
        </record>



    </data>
</odoo>