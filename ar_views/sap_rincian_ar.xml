<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="sap_rincianar_action" model="ir.actions.act_window">
            <field name="name">Rincian AR</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">sap.rincianar</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

        <record id="sap_rincianar_form" model="ir.ui.view">
            <field name="name">sap.rincianar.form</field>
            <field name="model">sap.rincianar</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Rincian AR">

                    <group string="Rincian AR"> 
                    
                         <field name="datefrom" /> 
                         <field name="dateto" /> 
                         <field name="partner" /> 
                         <field name="item" /> 
                    </group>
        
                    <footer>
                        <button string="View XLS" name="view_rincianar_xls" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel" />
                    </footer>

                </form>
            </field>
        </record>




    </data>
</odoo>