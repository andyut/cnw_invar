<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>


        <record id="sap_ar_home_tree" model="ir.ui.view">
            <field name="name">sap.ar.home.tree</field>
            <field name="model">sap.ar.home</field>
            <field name="arch" type="xml">
                <tree string="AR Dashboard">
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="sap_ar_home_form" model="ir.ui.view">
            <field name="name">sap.ar.home.form</field>
            <field name="model">sap.ar.home</field>
            <field name="arch" type="xml">
                <form string="AR Dashboard">
                    <field name="name"/>
                </form>
            </field>
        </record>

        <record id="sap_ar_home_action" model="ir.actions.act_window">
            <field name="name">AR Department</field>
            <field name="res_model">sap.ar.home</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p>
                    <center>
                        <strong>
                    AR Department
                        </strong>
                    </center>
                </p>
                <p>
                    AR Department Module
                </p>
            </field>
        </record>



    </data>
</odoo>