<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="cnw_cflwup_followup_report_wizard" model="ir.actions.act_window">
            <field name="name"> Follow Up Report </field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">cnw.cflwup.followup.report</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

        <record id="cnw_cflwup_followup_report_form" model="ir.ui.view">
            <field name="name">cnw.cflwup.followup.report.form</field>
            <field name="model">cnw.cflwup.followup.report</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Follow Up Report">

                    <group string="Follow Up Report"> 
 
                        <field name="export_to" /> 
                    </group>

                    <footer>
                        <button string="Generate Report" name="generate_report" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel" />
                    </footer>

                </form>
            </field>
        </record>




    </data>
</odoo>